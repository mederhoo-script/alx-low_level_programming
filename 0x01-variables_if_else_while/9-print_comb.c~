#include <stdio.h>

/**
 * main - program start
 *
 * combinations of single-digit numbers,
 *              separated by ", " and in ascending order.
 *
 * Return: Always 0
 */
int main(void)
{
	int i;
	
	putchar('0');

	for (i = 1; i <= 9; i++)
	{
		putchar(',');
		putchar(' ');
		putchar(i + '0');
	}

	putchar('\n');

	return (0);
}
