#include <stdio.h>

/**
 * add - add
 * @a: a
 * @b: b
 *
 * Return: int
 */
	int add(a, b)
	{
		return (a + b);
	}
/**
 * sub
 * @a: a
 * @b: b
 * Return: int
 */
	int sub(a, b)
	{
		return (a - b);
	}
/**
 * mul - multiply
 * mod - module
 * @a: a
 * @b: b
 * Return: int
 */
	int mul(a, b)
	{
		return (a *b);
	}
/**
 * div - module
 * @a: a
 * @b: b
 * Return: int
 */
	int div(a, b)
	{
		return (a / b);
	}
/**
 * mod - module
 * @a: a
 * @b: b
 * Return: int
 */
	int mod(a, b)
	{
		return (a % b);
	}
